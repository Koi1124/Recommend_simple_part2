<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane dividerPositions="0.8844221105527639" orientation="VERTICAL" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="wordcloud.WordCloudController">
  <items>
    <AnchorPane>
         <children>
            <ImageView fx:id="wordCloudImage" pickOnBounds="true" preserveRatio="true" />
         </children></AnchorPane>
    <AnchorPane>
         <children>
            <Button fx:id="save" mnemonicParsing="false" onAction="#saveClickedAction" text="保存词云" textAlignment="CENTER" textFill="#3e7ada">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children></AnchorPane>
  </items>
</SplitPane>
